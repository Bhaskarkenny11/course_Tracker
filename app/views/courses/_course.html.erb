<div id="<%= dom_id course %>">
  <p>
    <strong>Name:</strong>
    <%= course.name %>
  </p>

  <p>
    <strong>Instructor:</strong>
    <%=link_to course.instructor.name, instructor_path(course.instructor_id) %>
  </p>

  <p>
    <strong>Description:</strong>
    <%= course.description %>
  </p>
<h3>Enrolled Students</h3>
<ol>
<% course.students.each do |student|%>
<li><%=student.name%></li>
<%end%>
</ol>




</div>
